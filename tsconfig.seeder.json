{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  "types": ["node"],
  "forceConsistentCasingInFileNames": true
  },
  "include": [
    "seeder.ts",
    "src/models/**/*.ts",
    "src/data/**/*.ts",
    "src/lib/**/*.ts"
  ],
  "exclude": ["node_modules"]
}
